{
  "expo": {
    "name": "silversitting",
    "slug": "silverapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/brand/icon.png",
    "scheme": "silverapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.robiulalamdx.silversitting",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "splash": {
      "image": "./assets/brand/logo.png",
      "resizeMode": "native",
      "backgroundColor": "#FFFFFF"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/brand/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "icon": "./assets/brand/icon.png",
      "edgeToEdgeEnabled": true,
      "package": "com.robiulalamdx.silversitting",
      "build": {
        "minify": true,
        "shrinkResources": true
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/brand/icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-image-picker",
      [
        "expo-notifications",
        {
          "icon": "./assets/brand/icon.png",
          "color": "#FFFFFF",
          "defaultChannel": "default",
          "enableBackgroundRemoteNotifications": false
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a10d23f1-2e94-4793-9e91-87a2c861d005"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/a10d23f1-2e94-4793-9e91-87a2c861d005"
    }
  }
}