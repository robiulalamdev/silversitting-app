{
  "expo": {
    "name": "silversitting",
    "slug": "silverapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/brand/icon.png",
    "scheme": "silverapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "splash": {
      "image": "./assets/brand/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#8b3888"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/brand/splash.png",
        "backgroundColor": "#8b3888"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.robiulalamdx.silversitting",
      "build": {
        "minify": true,
        "shrinkResources": true
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/brand/splash.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/brand/splash.png",
          "backgroundColor": "#8b3888"
        }
      ],
      "expo-image-picker",
      [
        "expo-notifications",
        {
          "icon": "./assets/brand/logo.png",
          "color": "#8b3888",
          "defaultChannel": "default",
          "enableBackgroundRemoteNotifications": false
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a10d23f1-2e94-4793-9e91-87a2c861d005"
      }
    }
  }
}
