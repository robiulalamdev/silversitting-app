{
  "expo": {
    "name": "SilverSitting",
    "slug": "SSapplication",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/brand/icon.png",
    "scheme": "SSapplication",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/brand/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "icon": "./assets/brand/icon.png",
      "edgeToEdgeEnabled": true,
      "build": {
        "minify": true,
        "shrinkResources": true
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.robiulalamdx.SSapplication"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/brand/icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-image-picker",
      [
        "expo-notifications",
        {
          "icon": "./assets/brand/icon.png",
          "color": "#FFFFFF",
          "defaultChannel": "default",
          "enableBackgroundRemoteNotifications": false
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#FFFFFF",
          "image": "./assets/brand/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "dark": {
            "backgroundColor": "#FFFFFF",
            "image": "./assets/brand/splash.png",
            "resizeMode": "contain"
          },
          "android": {
            "backgroundColor": "#FFFFFF"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c4cf4aa1-b903-44e0-a67c-c620e2a287af"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}